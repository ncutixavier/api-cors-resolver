{
  "swagger": "2.0",
  "info": {
    "version": "1.0.0",
    "title": "LMS API",
    "description": "LMS API"
  },
  "servers": [
    {
      "url": "http://localhost:4000",
      "name": "localhost"
    },
    {
      "url": "https://leave-app-sys.herokuapp.com/",
      "name": "dev"
    }
  ],
  "paths": {
    "/": {
      "get": {
        "tags": ["Welcome"],
        "summary": "Get Started",
        "description": "Welcome Endpoint",
        "responses": {
          "200": {
            "description": "Welcome!"
          }
        }
      }
    },
    "/api/v1/users/register": {
      "post": {
        "tags": ["Users"],
        "summary": "Register a user",
        "description": "Register a user",
        "operationId": "register_user",
        "parameters": [
          {
            "in": "body",
            "name": "body",
            "schema": {
              "type": "object",
              "properties": {
                "name": {
                  "type": "string"
                },
                "phone": {
                  "type": "integer"
                },
                "email": {
                  "type": "string"
                },
                "title": {
                  "type": "string"
                },
                "department": {
                  "type": "string"
                },
                "gender": {
                  "type": "string"
                },
                "password": {
                  "type": "string"
                }
              }
            }
          }
        ],
        "produces": ["application/json"],
        "responses": {
          "201": {
            "description": "success"
          }
        }
      }
    },
    "/api/v1/coinmarket": {
      "get": {
        "tags": ["Coinmarket"],
        "summary": "Get Coinmarket",
        "description": "Get Coinmarket",
        "operationId": "get_coinmarket",
        "produces": ["application/json"],
        "responses": {
          "200": {
            "description": "success"
          }
        }
      }
    }
  }
